{
  "name": "pwa",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node server",
    "rimraf": "rm -rf public && mkdir -p public/assets",
    "prebuild": "npm run rimraf",
    "build": "npm run build:js && npm run build:sw",
    "build:js": "node build.js",
    "build:sw": "workbox generate:sw",
    "prewatch": "npm run rimraf",
    "watch": "run-p watch:*",
    "watch:client": "node build.js watch",
    "watch:server": "nodemon server",
    "test": "standard && npm run test-deps",
    "test-deps": "dependency-check . && dependency-check . server/index.js --extra --no-dev"
  },
  "dependencies": {
    "choo": "^6.11.0-preview1",
    "choo-async": "^0.1.1",
    "choo-bundles": "^0.2.1",
    "choo-data": "^0.1.0",
    "choo-service-worker": "^2.4.0",
    "choo-ssr": "^0.2.0",
    "cors": "^2.8.4",
    "fastify": "^1.0.0-rc.2",
    "fastify-static": "^0.10.0",
    "node-fetch": "^2.0.0",
    "rss-parser": "^3.1.1",
    "sanitize-html": "^1.18.2"
  },
  "devDependencies": {
    "bl": "^1.2.1",
    "browserify": "^16.1.0",
    "choo-devtools": "^2.3.3",
    "dependency-check": "^3.1.0",
    "exorcist": "^1.0.1",
    "from2-string": "^1.1.0",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.2",
    "sheetify": "^7.2.0",
    "split-require": "^3.1.0",
    "standard": "^10.0.3",
    "static-module": "^2.2.2",
    "through2": "^2.0.3",
    "tinyify": "^2.4.0",
    "watchify": "^3.10.0",
    "workbox-cli": "^2.1.2"
  },
  "engines": {
    "node": "9.8.x"
  }
}
